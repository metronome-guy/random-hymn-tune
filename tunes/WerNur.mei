              <staff n="1"><layer n="1">
                <note xml:id="m4_n1" pname="e" oct="4" dur="4"/>
                <note xml:id="m4_n2" pname="a" oct="4" dur="4"/>
                <note xml:id="m4_n3" pname="a" oct="4" dur="4"/>
                <note xml:id="m4_n4" pname="g" accid="s" oct="4" dur="4"/>
